{
  "name": "jamserve",
  "version": "0.1.0-alpha",
  "author": "ffalt",
  "license": "MIT",
  "description": "jamserve streaming audio server",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ffalt/git"
  },
  "bin": {
    "jamserve": "./dist/bundle.js"
  },
  "scripts": {
    "start": "node dist/bundle.js",
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:docs": "npm run dev:docs:api:jam && npm run dev:docs:api:subsonic",
    "develop": "concurrently --kill-others \"npm run dev:watch\" \"npm run dev:nodemon\"",
    "build:deploy": "npm run clean && npm run build:prod && npm run build:docs",
    "cmd:cleandb": "node dist/bundle.js --cleardb",
    "dev:nodemon": "nodemon --config nodemon.json",
    "dev:watch": "webpack --watch --mode development",
    "dev:lint": "tslint --project tsconfig.json \"src/**/*.ts\"",
    "dev:update:schema:jam": "cd dev/bin && ts-node update-jam-schema.ts",
    "dev:update:schema:subsonic": "cd dev/bin && ts-node update-subsonic-schema.ts",
    "dev:update:elasticmapping": "cd dev/bin && ts-node update-elastic-mapping.ts",
    "dev:update:openapi:jam": "cd dev/bin && ts-node update-jam-openapi.ts",
    "dev:update:openapi:subsonic": "cd dev/bin && ts-node update-subsonic-openapi.ts",
    "dev:update:client:jam": "cd dev/bin && ts-node update-jam-client.ts",
    "dev:update:router:jam": "cd dev/bin && ts-node update-jam-router.ts",
    "dev:update:router:subsonic": "cd dev/bin && ts-node update-subsonic-router.ts",
    "dev:update:jam": "npm run dev:update:schema:jam && npm run dev:update:openapi:jam && npm run dev:update:client:jam && npm run dev:update:router:jam",
    "dev:update:subsonic": "npm run dev:update:schema:subsonic && npm run dev:update:openapi:subsonic && npm run dev:update:router:subsonic",
    "dev:update": "npm run dev:update:jam && npm run dev:update:subsonic && npm run dev:update:elasticmapping",
    "dev:docs:api:subsonic": "redoc-cli bundle src/model/subsonic-openapi-1.16.0.json --output ./dist/docs/api/subsonic/index.html",
    "dev:docs:api:jam": "redoc-cli bundle src/model/jam-openapi-0.1.0.json --output ./dist/docs/api/jam/index.html",
    "clean": "rimraf dist",
    "test:mocha": "mocha --recursive test/test.ts test/**/test.ts",
    "test": "mocha --recursive src/**/*.spec.ts",
    "coverage": "nyc mocha --recursive test/test.ts test/**/test.ts"
  },
  "dependencies": {
    "ajv": "^6.5.5",
    "archiver": "^3.0.0",
    "body-parser": "^1.18.3",
    "colors": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "elasticsearch": "^15.2.0",
    "express": "^4.16.4",
    "express-rate-limit": "^3.3.2",
    "express-session": "^1.11.3",
    "feedparser": "^2.2.9",
    "fluent-ffmpeg": "^2.0.1",
    "fs-extra": "^7.0.1",
    "helmet": "^3.15.0",
    "jamp3": "^0.1.3",
    "jimp": "^0.5.6",
    "jsonwebtoken": "^8.4.0",
    "limiter": "^1.1.3",
    "md5-typescript": "^1.0.5",
    "mime-types": "^2.1.21",
    "moment": "^2.22.2",
    "multer": "^1.4.1",
    "multer-autoreap": "^1.0.3",
    "nedb": "^1.8.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "request": "^2.88.0",
    "svgo": "^1.1.1",
    "tmp": "0.0.33",
    "waveform-data": "^2.1.2",
    "winston": "^3.1.0",
    "winston-timer": "^0.1.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/archiver": "^2.1.2",
    "@types/chai": "^4.1.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/elasticsearch": "^5.0.28",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/feedparser": "^2.2.1",
    "@types/fluent-ffmpeg": "^2.1.8",
    "@types/fs-extra": "^5.0.4",
    "@types/helmet": "0.0.42",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/mime-types": "^2.1.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.5",
    "@types/multer": "^1.3.7",
    "@types/nedb": "^1.8.6",
    "@types/node": "^10.12.10",
    "@types/passport": "^0.4.7",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.1",
    "@types/rimraf": "^2.0.2",
    "@types/supertest": "^2.0.6",
    "@types/svgo": "^1.0.1",
    "@types/tmp": "0.0.33",
    "@types/xml2js": "^0.4.3",
    "ava": "1.0.0-rc.2",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "node-memwatch": "^1.0.1",
    "nodemon": "^1.18.7",
    "nyc": "^13.1.0",
    "redoc-cli": "^0.7.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.9",
    "supertest": "^3.3.0",
    "ts-loader": "5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "typescript": "^3.1.6",
    "typescript-json-schema": "^0.33.0",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "test/**/*.ts"
    ]
  },
  "nyc": {
    "temp-directory": "./reports/.nyc_output",
    "report-dir": "./reports/coverage",
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "engines": {
    "node": ">= 0.8.10"
  }
}
